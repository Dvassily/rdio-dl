{

    "manifest_version": 2,
    "name": "radiooooo-dl",
    "version": "0.1",

    "description": "Download music on radiooooo.",

    "icons": {
	"48": "icons/rdiodl-48.png"
    },
    
    "permissions": [
	"webRequest",
	"downloads",
	"runtime",
	"http://radiooooo.com/",
	"http://radiooooo.com/media-server/links/*"
    ],

    "background": {
	"scripts": ["background.js"]
    }
}
